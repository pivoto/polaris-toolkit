<?xml version="1.0" encoding="UTF-8"?>
<env>
	<outdir>../</outdir>
	<property>
		<entry key="jaskcon" value="false" />
		<entry key="mysql.useGeneratedKeys" value="true" />
		<entry key="src.dir" value="antengine-framework/src/main/java" />
		<entry key="web.dir" value="antengine-armchair-background/src/main/webapp" />
		<entry key="entity.dir" value="entity" />
		<entry key="mapper.dir" value="mapper" />
		<entry key="service.dir" value="service" />
		<entry key="service.impl.dir" value="service/impl" />
		<entry key="controller.dir" value="controller" />
	</property>

	<group>
		<template>
			<path>/vm/java-vo.vm</path>
			<outdir>${src.dir}/${tablePackage}/${entity.dir}</outdir>
			<filename>${tableJavaName}Entity.java</filename>
		</template>
		<template>
			<path>/vm/java-mapper.vm</path>
			<outdir>${src.dir}/${tablePackage}/${mapper.dir}</outdir>
			<filename>${tableJavaName}Mapper.java</filename>
		</template>
		<template>
			<path>/vm/java-mapper.xml.vm</path>
			<outdir>${src.dir}/${tablePackage}/${mapper.dir}</outdir>
			<filename>${tableJavaName}Mapper.sqlmap.xml</filename>
		</template>
		<template>
			<path>/vm/java-service.vm</path>
			<outdir>${src.dir}/${tablePackage}/${service.dir}</outdir>
			<filename>I${tableJavaName}Service.java</filename>
		</template>
		<template>
			<path>/vm/java-service.impl.vm</path>
			<outdir>${src.dir}/${tablePackage}/${service.impl.dir}</outdir>
			<filename>${tableJavaName}ServiceImpl.java</filename>
		</template>
		<template>
			<path>/vm/java-controller.vm</path>
			<outdir>${src.dir}/${tablePackage}/${controller.dir}</outdir>
			<filename>${tableJavaName}Controller.java</filename>
		</template>
		<template>
			<path>/vm/java-controller.vm</path>
			<outdir>${src.dir}/${tablePackage}/${controller.dir}</outdir>
			<filename>${tableJavaName}Controller.java</filename>
		</template>
		<template>
			<path>/vm/js-list.vm</path>
			<outdir>${web.dir}/resources/js/antengine/${tableClassify}/${tableXmlName}</outdir>
			<filename>list.js</filename>
		</template>
		<template>
			<path>/vm/jsp-list.vm</path>
			<outdir>${web.dir}/WEB-INF/view/${tableClassify}/${tableXmlName}</outdir>
			<filename>list.jsp</filename>
		</template>
		<template>
			<path>/vm/jsp-form.vm</path>
			<outdir>${web.dir}/WEB-INF/view/${tableClassify}/${tableXmlName}</outdir>
			<filename>form.jsp</filename>
		</template>

		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_version" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_menu" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_menu_auth" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_resource" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_resource_auth" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_role" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_user" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_user_auth" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_user_authc" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_user_info" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_login_info" catalog="antengine" schema="" />
		<config classify="sys" package="com.antengine.framework.base.sys" name="sys_log_info" catalog="antengine" schema="" />

		<config classify="general" package="com.antengine.armchair.general" name="place_info" catalog="antengine" schema="" />

	</group>
</env>
